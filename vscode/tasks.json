{
    "version": "2.0.0",
    "tasks": [
    {
        "label": "build current cuda file",
        "type": "shell",
        "command": "/usr/bin/nvcc",
        "args": [
            "-g",
            "-o",
            "${fileDirname}/${fileBasenameNoExtension}",
            "${file}",
        ],
        "group": {
        "kind": "build",
        "isDefault": true
    },
    "problemMatcher": ["$gcc"]
    }
        ]
}
